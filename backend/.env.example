# Supabase Configuration (Required)
SUPABASE_URL=your_supabase_url
# Use Service Role Key for backend (bypasses RLS - recommended)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
# Alternatively use Anon Key (requires RLS policies to allow authenticated backend access)
SUPABASE_ANON_KEY=your_supabase_anon_key

# Encryption Secret for API Keys (Required for storing user API keys)
# Generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
ENCRYPTION_SECRET=your_encryption_secret

# CORS Allowed Origins (comma-separated, for production)
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000
